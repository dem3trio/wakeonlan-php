{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
    <a onclick="wakeUp('{{ admin.generateObjectUrl('wakeUp', object) }}'); return false;" href="#" class="btn btn-sm btn-default view_link" title="{{ 'action_wake_up'|trans({}, 'SonataAdminBundle') }}">
        <i class="fa fa-eye"></i>
        {{ 'action_wake_up'|trans({}, 'SonataAdminBundle') }}
    </a>
{% endif %}
